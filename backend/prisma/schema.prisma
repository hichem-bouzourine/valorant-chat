model matchResult {
  id        String   @default(cuid()) @id
  team1     String
  team2     String
  score1    String
  score2    String
  flag1     String
  flag2     String
  time_completed String
  round_info String
  tournament_name String
  match_page String
  tournament_icon String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model upcomingMatches {
  id        String   @default(cuid()) @id
  team1     String
  team2     String
  score1    String
  score2    String
  flag1     String
  flag2     String
  time_until_match String
  round_info String
  tournament_name String
  match_page String
  tournament_icon String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}